{
  "name": "resoul/http-rest-api",
  "description": "http rest api",
  "minimum-stability": "stable",
  "license": "proprietary",
  "version": "0.1.0",
  "authors": [
    {
      "name": "resoul",
      "email": "resoul.ua@icloud.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "yiisoft/yii2": "~2.0.49.3",
    "ramsey/uuid": "~4.2.3",
    "yiisoft/yii2-symfonymailer": "~3.0.0",
    "yiisoft/yii2-queue": "^2.3.6",
    "firebase/php-jwt": "^6.10",
    "paragonie/sodium_compat": "^1.20",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "phpunit/phpunit": "^10.0",
    "codeception/codeception": "^5.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Middleware\\": "app/code/Middleware/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php bin/middleware migrate --interactive=0"
    ],
    "post-update-cmd": [
      "php bin/middleware migrate --interactive=0"
    ]
  },
  "config": {
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
